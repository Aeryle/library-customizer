{
  "name": "librarycustomizer",
  "version": "1.0.0",
  "description": "Easily customize your Library in some clicks!",
  "private": true,
  "main": "build/index.js",
  "scripts": {
    "copy": "cp -a src/*.html build; cp -a src/css build",
    "win:copy": "copy /Y src\\*.html build & mkdir build\\css & copy /L /Y src\\css build\\css",
    "move": "mv build/src/index.js build; rm -rf build/src",
    "win:move": "move build\\src\\index.js build & rd /S /Q build\\src",
    "compile": "rm -rf build; tsc -p tsconfig.json; npm run copy; webpack; npm run move",
    "win:compile": "rd /S /Q build & mkdir build & tsc -p tsconfig.json & npm run win:copy & webpack & npm run win:move",
    "prod:pack": "electron-builder -wl",
    "start": "electron ."
  },
  "build": {
    "files": [
      "build/*.*",
      "build/**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "executableName": "LibraryCustomizer",
      "target": [
        {
          "target": "tar.gz"
        },
        {
          "target": "AppImage"
        }
      ]
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "common-tags": "^1.8.0",
    "electron-debug": "^3.0.1",
    "safe-regex": "^2.1.1",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "@types/node": "^13.1.1",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "electron": "^7.1.7",
    "electron-builder": "^21.2.0",
    "eslint": "^6.8.0",
    "eslint-config-marine": "^5.3.2",
    "npx": "^10.2.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
